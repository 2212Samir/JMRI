  <decoder>
    <family name="Hans Deloof LocoIO" mfg="Public-domain and DIY">
      <model model="Basic LocoIO" productID="142">
          <versionCV lowVersionID="134" highVersionID="142"/>
      </model>
    </family>
    <programming direct="no" paged="no" register="no" ops="no">
        <mode>LOCONETSV1MODE</mode>
    </programming>
    <variables>

        <variable CV="7" readOnly="yes" item="Decoder Version" default="142">
            <decVal/>
            <label>PIC Version: </label>
            <label xml:lang="it">Versione PIC: </label>
            <label xml:lang="fr">Version PIC: </label>
            <label xml:lang="de">PIC Version: </label>
            <label xml:lang="nl">PIC-versie: </label>
        </variable>

        <variable CV="13" mask="XXXVVVVV" comment="Board Blink Rate" item="Blink Rate" default="0">
            <decVal min="0" max="15" default="0"/>
            <label>Blink Rate:</label>
            <tooltip>Choose from 0 = none to 15 = fast</tooltip>
        </variable>

        <variable CV="12" mask="VVVXXXXX" comment="Board Mode" item="Action Mode" default="0">
            <enumVal>
                <enumChoice choice="0" value="0">
                    <choice>Normal</choice>
                    <choice xml:lang="nl">Normaal</choice>
                </enumChoice>
                <enumChoice choice="1" value="2">
                    <choice>Alternate push button</choice>
                </enumChoice>
                <enumChoice choice="2" value="8">
                    <choice>Servo</choice>
                </enumChoice>
                <enumChoice choice="3" value="12">
                    <choice>4-Position Servo</choice>
                </enumChoice>
            </enumVal>
            <tooltip>Select how the board should operate</tooltip>
        </variable>

     <!-- This sets a single JMRI-style composite address in two CVs -->
      <variable CV="1" comment="Board SV address" item="Short Address" default="81">
        <splitVal highCV="2" factor="1" offset="-256" default="1"/>
        <label>Board Address (14 bit):</label>
        <tooltip>This provides the full address by combining the LocoIO Address upper and LocoIO Subaddress lower parts</tooltip>
      </variable>

      <!-- This repurposes the UserID fields on the Basic pane to show -->
      <!-- the individual address bytes.                               -->
      <!-- Note defaults must be present both locations and agree      -->
        <variable item="User Id #2" CV="2" default="81">
            <decVal min="1" max="127"/>
            <label>LocoIO Address (7-bit high)</label>
            <tooltip>Message DSTH byte</tooltip>
        </variable>

      <variable item="User Id #1" CV="1" default="1">
        <decVal max="127"/>
            <label>/ SubAddress (7-bit low):</label>
            <tooltip>Message DSTL byte</tooltip>
      </variable>

      <!-- per port config SVs, stored as CVs, use enum-HDL-PortMode ? -->
      <!-- Port 1 -->
      <variable item="Port1 Type" CV="3" mask="VVVVVVVV" default="192">
        <xi:include href="http://jmri.org/xml/decoders/public_domain/enum-HDL-PortMode.xml"/>
        <label>Port 1 Type:</label>
        <label xml:lang="nl">Poort 1 Type:</label>
      </variable>

      <variable item="Port1 Add1" CV="3" default="1">
        <hexVal/>
        <label>Port1 Add1</label>
      </variable>

      <variable item="Port1 Add2" CV="4" default="0">
        <hexVal/>
        <label>Port1 Add2</label>
      </variable>

      <variable item="Port1 Address" CV="4" mask="XXXXVVVV" default="1">
          <decVal max="255"/>
        <label>Port 1 Address (dec):</label>
        <label>Adres Poort 1 (dec):</label>
      </variable>

      <!-- Port 2 -->
      <variable item="Port2 Type" CV="6" mask="VVVVVVVV" default="192">
        <xi:include href="http://jmri.org/xml/decoders/public_domain/enum-HDL-PortMode.xml"/>
        <label>Port 2 Type:</label>
        <label xml:lang="nl">Poort 2 Type:</label>
      </variable>

      <variable item="Port2 Add1" CV="6" default="1">
        <hexVal/>
        <label>Port2 Add1</label>
      </variable>

      <variable item="Port2 Add2" CV="7" default="0">
        <hexVal/>
        <label>Port2 Add2</label>
      </variable>

      <variable item="Port2 Address" CV="7" default="1">
            <decVal max="255"/>
        <label>Port 2 Address (dec):</label>
        <label>Adres Poort 2 (dec):</label>
      </variable>

