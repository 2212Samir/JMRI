    <!--Mapping Line 1-->
    <!--Line 1 Mapping condition On-->
    <variable item="Z1 Direction Fw" CV="257" mask="VXXXXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Forward</label>
    </variable>
    <variable item="Z1 Moving Yes" CV="257" mask="XVXXXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Moving</label>
    </variable>
    <variable item="Z1 On Invert" CV="257" mask="XXVXXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Invert</label>
    </variable>
    <!--Line 1 Function Buttons On-->
    <variable item="Z1 F0 Cond On" CV="257" mask="XXXVXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <variable item="Z1 F1 Cond On" CV="257" mask="XXXXVXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <variable item="Z1 F2 Cond On" CV="257" mask="XXXXXXVX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <variable item="Z1 F3 Cond On" CV="257" mask="XXXXXVXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <!--Line 1 Mapping condition Off-->
    <variable item="Z1 Direction Rv" CV="263" mask="VXXXXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Reverse</label>
    </variable>
    <variable item="Z1 Moving No" CV="263" mask="XVXXXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Not moving</label>
    </variable>
    <variable item="Z1 Off Invert" CV="263" mask="XXVXXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Invert</label>
    </variable>
    <!--Line 1 Function Buttons Off-->
    <variable item="Z1 F0 Cond Off" CV="263" mask="XXXVXXXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
