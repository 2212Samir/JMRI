        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <variable item="Z1 F2 Cond Off" CV="263" mask="XXXXXXVX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <variable item="Z1 F3 Cond Off" CV="263" mask="XXXXXVXX" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
    </variable>
    <!--Line 1 Mapping Output-->
    <variable item="Z1 Out1" CV="269" default="0">
        <enumVal>
            <enumChoice choice="NONE">
                <choice>None</choice>
            </enumChoice>
            <enumChoice choice="AUX1">
                <choice>AUX1</choice>
            </enumChoice>
            <enumChoice choice="AUX2">
                <choice>AUX2</choice>
            </enumChoice>
            <enumChoice choice="AUX3">
                <choice>AUX3</choice>
            </enumChoice>
        </enumVal>
        <label>AUX</label>
    </variable>
    <!--Line 1 Mapping Output-->
    <variable item="Z1 Out3" CV="271" default="0">
        <enumVal>
            <enumChoice choice="NONE">
                <choice>None</choice>
            </enumChoice>
            <enumChoice choice="AUX1">
                <choice>AUX1</choice>
            </enumChoice>
            <enumChoice choice="AUX2">
                <choice>AUX2</choice>
            </enumChoice>
            <enumChoice choice="AUX3">
                <choice>AUX3</choice>
            </enumChoice>
        </enumVal>
        <label>Dimming 2</label>
    </variable>
    <variable item="Z1 Out4 Cross" CV="272" mask="XXXXXXXV" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Cross</label>
    </variable>
    <!--end of Line 1-->
</variables>
